}

@article{rasmussen_home_2012,
	title = {The Home Care Crew Scheduling Problem: Preference-based visit clustering and temporal dependencies},
	volume = {219},
	issn = {0377-2217},
	url = {http://www.sciencedirect.com/science/article/pii/S0377221711009891},
