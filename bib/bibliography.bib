
@inproceedings{sundar_exact_2015,
	title = {An exact algorithm for a heterogeneous, multiple depot, multiple traveling salesman problem},
	doi = {10.1109/ICUAS.2015.7152311},
	abstract = {Unmanned aerial vehicles are being used in several monitoring applications to collect data from a set of targets. These vehicles are heterogeneous in the sense that they can differ either in their motion constraints or sensing capabilities. Furthermore, not all vehicles may be able to visit a given target because vehicles may occasionally be equipped with disparate sensors due to the respective payload restrictions. This article addresses a problem where a group of heterogeneous vehicles located at distinct depots visit a set of targets. The targets are partitioned into disjoint subsets: targets to be visited by specific vehicles and targets that any of the vehicles can visit. The objective is to find an optimal tour for each vehicle starting at its respective depot such that each target is visited at least once by some vehicle, the vehicle-target constraints are satisfied and the sum of the costs of the tours for all the vehicles is minimized. We formulate the problem as a mixed-integer linear program and develop a branch-and-cut algorithm to compute an optimal solution to the problem. Computational results show that optimal solutions for problems involving 100 targets and 5 vehicles can be obtained within 300 seconds on average, further corroborating the effectiveness of the proposed approach.},
	eventtitle = {2015 International Conference on Unmanned Aircraft Systems ({ICUAS})},
	pages = {366--371},
	booktitle = {2015 International Conference on Unmanned Aircraft Systems ({ICUAS})},
	author = {Sundar, Kaarthik and Rathinam, Sivakumar},
	date = {2015-06},
	keywords = {Approximation algorithms, autonomous aerial vehicles, branch-and-cut, branch-and-cut algorithm, disjoint subsets, heterogeneous multiple depot, heterogeneous vehicles, Heuristic algorithms, integer programming, linear programming, Linear programming, mixed-integer linear program, Monitoring, monitoring applications, motion constraints, motion control, multiple traveling salesman problem, optimal control, optimal solutions, optimal tour, Routing, sensing capabilities, sensors, site-dependent vehicle routing, traveling salesman problem, Traveling salesman problems, travelling salesman problems, tree searching, unmanned aerial vehicles, unmanned vehicles, vehicle-target constraints, Vehicles},
	file = {IEEE Xplore Abstract Record:/home/jonas/snap/zotero-snap/common/Zotero/storage/RYA5RBHA/7152311.html:text/html}
}

